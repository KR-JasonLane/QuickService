<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:vm="clr-namespace:QuickService.ViewModels;assembly=QuickService.ViewModels"
                    xmlns:v="clr-namespace:QuickService.Views;assembly=QuickService.Views"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--                                                                -->
    <!--                            MainView                            -->
    <!--                                                                -->

    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <v:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TitleViewModel}">
        <v:TitleView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InteractionViewModel}">
        <v:InteractionView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SelectedFileViewModel}">
        <v:SelectedFileView/>
    </DataTemplate>
    
</ResourceDictionary>